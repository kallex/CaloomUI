		<Operations>
			<Operation name="UpdatePageContent">
				<OperationSpec>
					<Description/>
				</OperationSpec>
				<Parameters>
					<Parameter dataType="string" name="changedInformation" designDesc="Test" state="underDesign"/>
				</Parameters>
				<Execution>
					<SequentialExecution>
						<MethodExecute name="UpdatePage" designDesc="Do the update page" state="underDesign"/>
					</SequentialExecution>
				</Execution>
			</Operation>
